d={'a':1,'b':2,'c':3,'d':4,'e':5}
d1={k:v*2 for (k,v) in d.items()}
print(d1)



student_marks={
    'sita':95,
    'aakash':80,
    'anvesh':56,
    'honey':41,
    'mallesh':15,
    'arjun':54,
    'mani':29
}
dict={}
for student in student_marks:
    marks=student_marks[student]
    if marks>90:
        dict[student]='A+'
    elif marks>80:
        dict[student]='A'
    elif marks>70:
        dict[student]='B+'
    elif marks>60:
        dict[student]='C+'
    elif marks>50:
        dict[student]='c'
    else:
        dict[student]='F'
print(dict)



n=int(input("enter the number:"))
d = n//3
e=n//5
f=n//15
g=d+e-f
print(g)
