i=0
a=0
num=int(input("enter the number:"))
while num!=0:
    d=num%10
    s=d*10**i
    a+=s
    s=0
    i+=1
    num//=10
print(a)


armstrong
num=int(input("enter the number:"))
temp1=num
temp2=num
c=0
sum=0
while num!=0:
    num//=10
    c+=1
for i in range(1,c+1):
    a=temp1%10
    sum+=a**c
    temp1//=10
if sum==temp2:
    print("armstrong")
else:
    print("not a armstrong")



neon
sum=0
num=int(input("enter the number:"))
a=num*num
while a!=0:
    d=a%10
    sum+=d
    a//=10
if num==sum:
    print("neon")
else:
    print("not neon")


#magic
num=int(input("enter the number:"))
while num>9:
    sum=0
    while num!=0:
        d=num%10
        sum+=d
        num//=10
    num=sum
if num==1:
    print("magic")
else:
    print("not a magic")
            
    


composite factors
num=int(input("enter the number: "))
c=0
for i in range(1,num+1):
    if num%i==0:
        c+=1
if c>2:
    print("composite ")
else:
    print("not a composite")
    
            
       
perfect number
num=int(input("enter the number:"))
c=0
sum=0
for i in range(1,num):
    if num%i==0:
        sum+=i
if num==sum:
    print("perfect number")
else:
    print("not a perfect") 


strong
fact=1
sum=0
num=int(input("enter the number:"))
temp1=num
while num!=0:
    d=num%10
    for i in range(1,d+1):
        fact*=i
    sum+=fact
    fact=1
    num//=10
if temp1==sum:
    print("strong")
else:
    print("not a strong")
    
  
  
niven
num=int(input("enter the number:"))
sum=0
while num!=0:
    d=num%10
    sum+=d
    num//=10
if num%sum==0:
    print("niven")
else:
    print("not a niven") 


sum of  numbers==product of numbers(spy number?)
num=int(input("enter the number:"))
sum=0
mul=1
while num!=0:
    d=num%10
    sum+=d
    mul*=d
    num//=10
if sum==mul:
    print("spy")
else:
    print("not a spy")




list comprehsion
